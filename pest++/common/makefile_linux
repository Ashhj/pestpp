OUT = libcommon.a
OBJECTS	:= network_package.o \
           pest_error.o \
           Transformable.o \
           network_wrapper.o \
           system_variables.o \
           utilities.o \
           model_interface.o

$(OUT): $(OBJECTS)
	ar rcs $(OUT) $(OBJECTS)

%.o: %.cpp
	$(CXX) $(CFLAGS) $(INCLUDES) $< -c $(input) -o $@

%.o: %.c
	$(CC) $(CFLAGS) $(INCLUDES) $< -c $(input) -o $@

clean:
	-rm $(OBJECTS) $(OUT)